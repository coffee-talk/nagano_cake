<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜き価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td><%= link_to product.name, admins_product_path(product) %></td>
        <td><%= product.price %></td>
        <td><%= product.genre_id %></td>
        <td><%= product.is_active %></td>
      </tr>
    <% end %>
  </tbody>
</table>