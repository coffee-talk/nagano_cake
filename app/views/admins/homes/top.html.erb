<h2>注文履歴一覧</h2>
    <table>
      <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
      </thead>
      <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td><%= link_to order.created_at, admins_order_path(order) %></td>
          <td><%= order.name %></td>
          <td><%= order.amount %></td>
          <td><%= order.status %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
<div>
　<%= paginate @orders %>
</div>