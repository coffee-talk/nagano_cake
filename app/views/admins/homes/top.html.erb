<div class="contain">
 <div class="row">
  <div class="mx-auto col-12 col-xl-10">
<h2><span class="badge badge-secondary">注文履歴一覧</span></h2>
</div>
</div>
<div class= "col-md-8 offset-md-1">
<table class="table">
      <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
      </thead>
      <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td><%= link_to order.created_at, admins_order_path(order) %></td>
          <td><%= order.name %></td>
          <td><%= order.ordered_product.amount %></td>
          <td><%= order.status %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
<div>
　<%= paginate @orders %>
</div>
</div>
</div>